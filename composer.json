{
  "name": "tenantcloud/php-stax-client",
  "type": "library",
  "description": "Stax PHP Library",
  "keywords": [
    "stax",
    "payment processing",
    "api"
  ],
  "homepage": "https://github.com/tenantcloud/php-stax-client",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrii Tershak",
      "email": "a.tershak@tenantcloud.com",
      "homepage": "https://www.tenantcloud.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : ">=7.4",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": ">=8.0"
  },
  "autoload": {
    "psr-4": {
      "Stax\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Stax\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "cs-fix": "./tools/php-cs-fixer/vendor/bin/php-cs-fixer fix -v --show-progress=estimating"
  }
}
